extends ../../layouts/default.pug

include ../../mixins/alert.pug
include ../../mixins/box-head.pug

block main
    +alert-error(3500)
    
    .container.my-3
        include ../../partials/list-btn-users.pug 
        .row
            .col-12
                +box-head('Loi moi ket ban')

        .row(data-users-accept=user.id)
            each user in users
                .col-6(user-id=user.id)
                    .box-user
                        .inner-avatar
                            img(
                                src=(user.avatar || 'data:image/jpeg;base64,')
                                alt=user.fullname
                            )
                        .inner-info
                            .inner-name #{user.fullname}
                            .inner-buttons
                                button(
                                    class='btn btn-sm btn-primary mr-1'
                                    btn-accept-friend=user.id
                                ) Chap nhan
                                button(
                                    class='btn btn-sm btn-secondary mr-1'
                                    btn-refuse-friend=user.id
                                ) Xoa
                                button(
                                    class='btn btn-sm btn-secondary mr-1'
                                    btn-deleted-friend
                                    disabled
                                ) Da xoa
                                button(
                                    class='btn btn-sm btn-primary mr-1'
                                    btn-accepted-friend
                                    disabled
                                ) Da chap nhan
                            
    block script 
        script(type='module' src='/js/users.js')